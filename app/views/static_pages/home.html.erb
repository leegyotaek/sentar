<div class="content_re">
  <div class="row">
    <!-- blog side 김수겸 14.09.19 -->
    <div class="col-lg-2">
      <div class="left_wrap">
        <!--profile 김수겸 14.09.18-->
        <%= render 'users/profile'%>
        <!--profile end-->

        <!--blog_map js 김수겸 14.09.16-->
        <script>
        $(function() {
          var state = true;
          $("#spread_btn").click(function() {
            if (state) {
              $("#map_div").animate({
                height : 300
              }, 500);
            } else {
              $("#map_div").animate({
                height : 200
              }, 500);
            }
            state = !state;
          });
        });
        </script>
        <!--blog_map js end-->

        <!--blgo_map 김수겸 14.09.18-->
        <div class="row">
          <div class="col-lg-12">
            <div id="map_div" class="blog_map">
              <span class="spread_btn"><i id="spread_btn"class="fa fa-plus-square-o"></i></span>
            </div>
          </div>
        </div>
        <!--blog_map end-->
      </div>
    </div>
    <!-- blog side end -->

    <div class = "row">
      <%= render @products %>
    </div>
<script>
var blog_map_call = function(){
  map_env();
  blog_map();
}
$(blog_map_call);
</script>



