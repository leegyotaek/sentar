<div class="content_re">
  <div class="row">
    <!-- all_user side 김수겸 14.09.19 -->
    <div class="col-lg-2">
      <div class="left_wrap">
        <!--profile 김수겸 14.09.18-->
      <%= render 'users/profile'%>
        <!--profile end-->        
      </div>
    </div>
    <!-- all_user side end -->

    <!--all_user_list_wrap 김수겸 14.09.19-->
